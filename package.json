{
  "name": "@scbd/angular-vue",
  "version": "5.0.0-alpha.1",
  "description": "",
  "type": "module",
  "main": "dist/cjs/index.js",
  "import": "dist/esm/index.js",
  "unpkg": "dist/index.js",
  "jsdelivr": "dist/index.js",
  "browser": "dist/index.js",
  "files": [
    "dist/*",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf ./dist && semistandard && rollup -c",
