    "angular"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scbd/angular-vue/issues"
  },
  "homepage": "https://github.com/scbd/angular-vue#readme",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "lodash-es": "^4.17.21",
    "rollup": "^2.79.1",
    "semistandard": "^17.0.0",
    "snazzy": "^9.0.0",
    "vue": "^3.4.15"
  },
  "peerDependencies": {
    "vue": "^3.4.15"
  },
  "semistandard": {
